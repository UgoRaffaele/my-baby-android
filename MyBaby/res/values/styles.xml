<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Material.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:colorPrimary">#AA66CC</item>
	    <item name="android:colorPrimaryDark">#9933CC</item>
	    <item name="android:textColorPrimary">#FFFFFF</item>
	    <item name="android:navigationBarColor">#9933CC</item>
	    <item name="android:colorAccent">#AA66CC</item>
	    <item name="android:colorControlHighlight">#9933CC</item>
	    <item name="android:textColor">#FF9933CC</item>
    </style>

</resources>
